<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.itcast.bos.domain.Workbill" table="qp_workbill" schema="zjs_bos">
        <id name="id" column="id"/>
        <property name="type" column="type"/>
        <property name="pickstate" column="pickstate"/>
        <property name="buildtime" column="buildtime"/>
        <property name="attachbilltimes" column="attachbilltimes"/>
        <property name="remark" column="remark"/>
        <many-to-one name="noticebill" class="cn.itcast.bos.domain.Noticebill">
            <column name="noticebill_id" />
        </many-to-one>
        <many-to-one name="staff" class="cn.itcast.bos.domain.Staff">
            <column name="staff_id" />
        </many-to-one>
    </class>
</hibernate-mapping>